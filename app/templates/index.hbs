<h1 class="page-header">Public databases</h1>
<div class="list-group">
  {{#each model.known_databases as |entry|}}
    {{#link-to 'db.list' entry.name class="list-group-item"}}
      <h4 class="list-group-item-heading">{{entry.title}}</h4>
      <p class="list-group-item-text">{{entry.description}}</p>
    {{/link-to}}
  {{/each}}
</div>
<h1 class="page-header">Start your own</h1>
